{
  "name": "benjamin-michaelis-photography-portfolio",
  "private": true,
  "description": "Benjamin Michaelis's Photography Portfolio",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "cross-env CYPRESS_SUPPORT=y gatsby develop",
    "dev": "gatsby develop -o",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write content/**/*.yaml",
    "type-check": "tsc --noEmit",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=results/cypress/result.xml'",
    "server:open": "start-server-and-test develop http://localhost:8000 cy:open",
    "server:run": "start-server-and-test develop http://localhost:8000 cy:run",
    "server:ci": "start-server-and-test develop http://localhost:8000 cy:run:ci",
    "test:compile": "tsc --project cypress/tsconfig.json",
    "test:compile:watch": "tsc --watch --project cypress/tsconfig.json",
    "test": "npm-run-all -s test:compile server:run",
    "test:watch": "npm-run-all -p test:compile:watch server:open",
    "test:ci": "npm-run-all -s test:compile server:ci"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^2.1.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "gatsby": "^4.25.7",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-google-analytics": "^4.25.0",
    "gatsby-plugin-google-gtag": "^4.25.0",
    "gatsby-plugin-manifest": "^4.25.0",
    "gatsby-plugin-netlify": "^5.1.0",
    "gatsby-plugin-offline": "^5.25.0",
    "gatsby-plugin-react-helmet": "^5.25.0",
    "gatsby-plugin-sharp": "^4.25.1",
    "gatsby-plugin-sitemap": "^5.25.0",
    "gatsby-plugin-styled-components": "^5.25.0",
    "gatsby-plugin-typescript": "^4.24.0",
    "gatsby-source-filesystem": "^4.25.0",
    "gatsby-transformer-sharp": "^4.25.0",
    "gatsby-transformer-yaml": "^4.25.0",
    "node-fetch": "^3.3.0",
    "node-forge": "^1.3.1",
    "polished": "^4.2.2",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-spring": "^9.7.0",
    "styled-components": "^6.0.2",
    "styled-system": "^5.1.5",
    "trim": "^1.0.1",
    "typeface-work-sans": "^1.1.13",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.1",
    "@types/react-helmet": "^6.1.5",
    "@types/styled-components": "^5.1.21",
    "@types/styled-system": "^5.1.15",
    "@types/testing-library__cypress": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^5.59.0",
    "cross-env": "^7.0.2",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "start-server-and-test": "^2.0.0"
  },
  "resolutions": {
    "cypress": "^4.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BenjaminMichaelis/photography-website"
  },
  "bugs": {
    "url": "https://github.com/BenjaminMichaelis/photography-website/issues"
  }
}
